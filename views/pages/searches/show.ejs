<!DOCTYPE html>
<html lang="en">

    <html lang="en">
        <head>
            <title>Search Results</title>
            <link rel="stylesheet" href="/styles/base.css" />
        </head>
        <body>
            <h1>Search Results</h1>
        
        
            <% info.forEach(object =>{ %>
                <img src = <%= object.image %>>
                <div> 
                    <%= object.title %>
                </div>
                <div>
                    <%= object.author %>
                </div>
                <div>
                    <%= object.description %>
                </div>
            <% }) %>
        </body> 
        
    </html>
